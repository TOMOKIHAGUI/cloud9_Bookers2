<%= form_with url: search_path, method: :get, local: true do |f| %>
  <%= f.text_field :word %>
  <%= f.select :option, options_for_select([["User","1"],["Book","2"]]) %>
  <%= f.select :choice, options_for_select([["部分一致", "1"],["完全一致", "2"],["前方一致", "3"],["後方一致", "4"]]) %>
  <%= f.submit '検索' %>
<% end %>